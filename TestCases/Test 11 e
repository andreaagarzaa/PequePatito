// Uso de Temporales en Funciones
programa TestTemporaries;
vars
    a: entero;

nula compute() {
    vars
        temp1: entero;
    {
        temp1 = 100;
        escribe("Temporal dentro de compute: ", temp1);
    }
};

inicio{
    a = 10;
    compute();
    escribe("Variable 'a' despu√©s de compute: ", a);
}fin
