// Test:  Llamada a Funci칩n con Par치metros Incorrectos (Tipos)
// Verifica que el compilador detecte cuando se llama a una funci칩n con par치metros de tipos incorrectos

programa TestFuncParamTypes;
vars
    a: entero;

nula printSum(x: entero, y: entero) {
    vars
        sum: entero;
    {
        sum = x + y;
        escribe("Suma: ", sum);
    }
};

inicio{
    a = 10;
    printSum(a, 23.2); // Tipo incorrecto para 'y'
}fin
